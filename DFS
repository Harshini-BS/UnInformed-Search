class Node:
    def __init__(self, value):
        self.data = value
        self.left = None
        self.right = None


def dfs(node, result):
    if node is None:
        return
    result.append(str(node.data))
    dfs(node.left, result)
    dfs(node.right, result)


root = Node(1)
root.left = Node(2)
root.right = Node(3)
root.left.left = Node(4)
root.left.right = Node(5)
root.right.right = Node(6)


# Call DFS and collect output
result = []
dfs(root, result)


# Join and print nicely
print("DFS (Preorder Traversal):")
print("->".join(result))



OUTPUT:


DFS (Preorder Traversal):
1->2->4->5->3->6
